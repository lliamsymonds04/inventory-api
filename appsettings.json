{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost,1433;Database=InventoryApi;User Id=SA;Password={DatabasePassword};MultipleActiveResultSets=true;TrustServerCertificate=true"
  },
